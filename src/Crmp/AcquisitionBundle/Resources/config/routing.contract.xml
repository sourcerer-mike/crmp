<?xml version="1.0" encoding="UTF-8" ?>
<routes xmlns="http://symfony.com/schema/routing"
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:schemaLocation="http://symfony.com/schema/routing
        http://symfony.com/schema/routing/routing-1.0.xsd">

    <route id="crmp_acquisition_contract_delete" path="/{id}" methods="DELETE">
        <default key="_controller">CrmpAcquisitionBundle:Contract:delete</default>
    </route>

    <route id="crmp_acquisition_contract_show" path="/{id}" methods="GET">
        <default key="_controller">CrmpAcquisitionBundle:Contract:show</default>
    </route>

    <route id="crmp_acquisition_contract_edit" path="/{id}/edit" methods="GET,POST">
        <default key="_controller">CrmpAcquisitionBundle:Contract:edit</default>
    </route>

    <route id="crmp_acquisition_contract_index" path="/" methods="GET">
        <default key="_controller">CrmpAcquisitionBundle:Contract:index</default>
    </route>

    <route id="crmp_acquisition_contract_new" path="/new" methods="GET,POST">
        <default key="_controller">CrmpAcquisitionBundle:Contract:new</default>
    </route>
</routes>